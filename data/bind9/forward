@       IN      SOA     sid.%%zone%%. root.%%zone%%. (
                   2007010401           ; Serial
                         3600           ; Refresh [1h]
                          600           ; Retry   [10m]
                        86400           ; Expire  [1d]
                          600 )         ; Negative Cache TTL [1h]
;
@       IN      NS      sid.%%host%%.

sid     IN      A       %%local_ip%%
etch    IN      A       %%local_ip%%